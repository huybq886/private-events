<h1>HOME</h1>

<% @current_user = current_user %>
<% if !@current_user %>
  <h3>All Users:</h3>
  <ul>
    <% @users.each do |user| %>
      <li>
        <%= user.name %>
      </li>
    <% end %>
  </ul>
<% else %>
  <div>
    <p><%= link_to "Visit Personal Page", user_path(@current_user) %></p>
  </div>
<% end %>

<div>
  <p><%= link_to "All Events", 'events#index' %></p>
</div>